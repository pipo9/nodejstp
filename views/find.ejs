 

<div class="card service">
    <h3 class="">Uplaoded Posters</h3>
</div>

<div class="grid-container">

    <% items.forEach(function(image) { %> 

        <div class="card">
            <img class="card-img-top gallery_img" src="data:image/<%=image.img.contentType%>;base64, 
            <%=image.img.data.toString('base64')%>" alt="Card image">
          </div>

    <% }) %> 

</div> 


